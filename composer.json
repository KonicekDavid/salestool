{
	"name": "konicekdavid/salestool",
	"description": "Small sales tool",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/di": "^3.2",
		"nette/http": "^3.3",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"tracy/tracy": "^2.10",
		"contributte/api-router": "^6.0",
		"dibi/dibi": "^5.0",
		"nette/schema": "^1.3"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "^2.0",
		"phpstan/extension-installer": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true,
			"phpstan/extension-installer": true
		}
	},
	"scripts": {
		"analyse": "vendor/bin/phpstan analyse app -c config/phpstan.neon"
	}
}
